/* CSS file for the 45 10 45 stacked layout */
@import url('base-grid.css');

.panel-col-fourtyfive {
  width: 45%;
}
.panel-col-ten {
  width: 7%;
}

.panel-header, .panel-footer {
  width: 100%;
}

/* Margins taken from the Panels module plugins */
.lastUnit .inside {
  margin: 0 0 1em .5em;
}
.firstUnit .inside {
  margin: 0 .5em 1em 0;
	padding: 15px;
	background: #fff;
	border: 1px solid #D1D1CD;
}
.unit .inside {
  margin: 0 .5em 1em .5em;
}


.words, .tags, .comments, .comments_all_cloud, .annotation_count{
	margin:0px;
	padding:15px;
	background: #fff;
	margin:0 auto;
}

.words a, .tags a, .comments a, .comments_all_cloud a, .annotation_count a{
	display:block;
	height:35px;
	width:35px;
	/*border: #DDDDD4 2px solid;*/
}

.words a{
	background: transparent url('img/word_cloud.png') 1px 0px no-repeat;
}

/*.words a:hover{
	background: #DDD url('img/word_cloud.png') 10px 0px no-repeat;
}*/

#tabs-icons .ui-state-active.words a, 
#tabs-icons .ui-state-active.words a:link, 
#tabs-icons .ui-state-active.words a:visited{
	color:#000;
	/*background: #DDD url('img/word_cloud.png') 10px 0px no-repeat;*/
	background: #fff url('img/word_cloud.png') 1px 0px no-repeat;
}



.tags a{
	background: transparent url('img/tag_cloud.png') 1px 0px no-repeat;
}

li.tags a p,
li.comments a p{
	color: #D00;
	position: absolute;
	top: 32px;
	text-align: center;
	margin: 0px auto;
	width: 23px;
}



li.tags a{
	/*color:#F44365;*/
	color: #AC1A2F;
}

li.comments a p{
	color:#69F;
}

li.annotation_count a p{
	color:#000;
	margin:0;
}


#pane-community-tags-0 a{
