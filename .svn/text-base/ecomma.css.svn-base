  margin-bottom: 4px;
}

div.ec-right-bar ol a
{
  float: left;
  margin-right: 4px;
}

div.ec-right-bar ol img
{
  float: left;
  margin: 1px 0px 0px 3px;
}

div.ec-right-bar ol span
{
  float: right;
}

