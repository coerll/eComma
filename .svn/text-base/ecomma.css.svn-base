/* ****************************************************************************
 * Adjustments to the HRC column widths.
 * ****************************************************************************
 */

#content .three-column .left
{
  /*width: 380px;*/
	width:600px;
	margin-top:20px;
}

#content .three-column .left.ec-wide
{
  width: 600px;
}

#content .three-column .middle
{
  width: 180px;
	/*margin-top:20px;
  margin-left: 400px;*/
}

/* ****************************************************************************
 * General styles for elements.
 * ****************************************************************************
 */

cite
{
  font-style: italic;
}

/* ****************************************************************************
 * All-purpose classes for formatting.
 * ****************************************************************************
 */

.ec-warning
{
  color: #ac1a2f;
	font-size:14px;
	padding-top:10px;
}

.ec-hi
{
  padding: 4px;
  background-color: #ffff99;
  color: #ac1a2f;
}

.Partner1{
 background: #4C9E6D; /*69967C 393 green  rgb(51, 153, 51)*/
 color: #fff;
 
}

.Partner2{
 background: #DD9090;/* #E3B0B8; red rgb(227, 176, 184)*/
 color: #fff;
}

.Partners{
 background: #D89AD2; /*AD53A0 purple rgb(173, 83, 160)*/
 color: #fff;
}

.Partner-all{
 background: #895B5B; /* green  rgb(51, 153, 51)*/
 color: #fff;
}

.not_FE
{
  background: #ffff99;
  color: #ac1a2f;
}



.Partner1:hover,
.Partner2:hover,
.Partner3:hover{
	color: #ccc;
}

.ec-diff
{
  margin: 0em -1px;
  padding: 4px 1px;
  background-color: #ffff99;
  color: #ac1a2f;
}

.ec-em
{
  font-style: italic;
}

.ec-sc
{
  font-variant: small-caps;
}

div.ec-indent
{
  /*padding-left: 1em;*/
}

.ec-inactive,
.ec-note-inactive,
.ec-tag-inactive
{
  background-color: #e3e1d1;
}

.ec-form-message
{
  clear: both;
  color: #ac1a2f;
  margin-bottom: 6px;
}

/* ****************************************************************************
 * All-purpose classes for layout.
 * ****************************************************************************
 */

.ec-clear
{
  clear: both;
}

.ec-block
{
  clear: both;
  margin-bottom: 4px;
}

div.white-box div.inside div.ec-message
{
  font-size: 13px;
  color: #ac1a2f;
  margin-bottom: 11px;
}

/* ****************************************************************************
 * Styles for about pages.
 * ****************************************************************************
 */

div.ec-about p,
div.ec-about ol li
{
  font: normal 13px/18px Arial, sans-serif;
}

#content div.ec-about h3.boxed
{
  margin-bottom: 18px;
  font: bold 25px Arial, sans-serif;
	color:#453b26;
	padding: 11px 0px;
}

div.ec-about h4
{
  color: #453b26;
  font: bold 13px/18px Arial, sans-serif;
  padding-top: 15px;
	padding-bottom: 15px;
	border-top: 1px solid #89877D;
}

div.ec-about ol
{
  margin: 12px 0px 12px 12px;
}

div.ec-about ol li
{
  margin-bottom: 2px;
}

div.ec-about div.ec-image
{
  margin: 18px 0px;
}

div.ec-about div.ec-image img
{
  margin: 8px auto;
  /*display: block;*/
  padding: 2px;
  color: #5a594f;
  border: 1px solid #d7d5c2;
}

div.ec-about div.ec-image span
{
  /*font-weight: bold;*/
  display: inline;
  /*display: block;
  text-align: center;*/
	color:#998F7F;
  text-align: left;
	margin-top:10px;
	float:right;
	width:130px;
}

span.ec-link-example
{
  font: bold 10px Arial, sans-serif;
  padding: 0px 2px;
  text-transform: uppercase;
  text-decoration: none;
  color: #a17500;
}

div.ec-about ol.ec-credits
{
  padding-left: 18px;
}

div.ec-about ol.ec-credits li
{
  text-indent: -18px;
}

div.ec-about ol.ec-credits li span
{
  font-weight: bold;
}


/* ****************************************************************************
 * The right sidebar.
 * ****************************************************************************
 */
div.ec-right-bar
{
  padding-bottom: 6px;
}

#content div.ec-right-bar p
{
  margin-bottom: 8px;
}


div.white-box div.inside div.ec-right-bar h3
{
  font: normal 10px Arial, sans-serif;
  margin-bottom: 0px;
  padding-bottom: 6px;
}

