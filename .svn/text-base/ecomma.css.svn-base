/* ****************************************************************************
 * Tag clouds.
 * ****************************************************************************
 */

ol.tag-cloud
{
  font-family: Georgia, sans-serif;
  margin: 0em 0em;
  padding: 12px 0em;
  list-style-type: none;

  border-width: 1px;
  border-style: solid none none none;
  border-color: #e3e1d1;

  text-align: justify;
}

ol.tag-cloud > li
{
  display: inline;
  padding: 0px;
  line-height: 2em;
}

ol.tag-cloud > li > a
{
  color: #ac1a2f;
  padding: 0em 0.25em;
  text-decoration: none;
}

ol.tag-cloud > li > a.m-0 { font-size: 0.8em; }
ol.tag-cloud > li > a.m-1 { font-size: 0.9em; }
ol.tag-cloud > li > a.m-2 { font-size: 1.0em; }
ol.tag-cloud > li > a.m-3 { font-size: 1.2em; }
ol.tag-cloud > li > a.m-4 { font-size: 1.4em; }
ol.tag-cloud > li > a.m-5 { font-size: 1.6em; }
ol.tag-cloud > li > a.m-6 { font-size: 1.8em; }
ol.tag-cloud > li > a.m-7 { font-size: 2.0em; }
ol.tag-cloud > li > a.m-8 { font-size: 2.2em; }
ol.tag-cloud > li > a.m-9 { font-size: 2.5em; }

